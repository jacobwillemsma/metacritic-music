{
    "name": "MetaMusic",
    "version": "1.0.0",
    "description": "An extension that inserts the metacritic score of the album you're listening to in Google Music.",
    "background": {
        "scripts": ["src/js/background.js"]
    },
    "icons": { 
        "16": "icons/icon16.png",
        "48": "icons/icon48.png",
        "128": "icons/icon128.png"
    },
    "permissions": [
        "tabs",
        "http://*/",
        "https://*/"
    ],
    "manifest_version": 2
}
